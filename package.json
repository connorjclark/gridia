    "uuid": "^8.3.2",
    "wrtc": "^0.4.7",
    "ws": "*",
    "yargs": "^13.2.2"
  },
  "resolutions": {
    "pixi-sound/@pixi/loaders": "6.0.1"
  },
  "engines": {
    "node": "16.10.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "yarn build-client && yarn build-server",
    "build-prod": "NODE_ENV=production yarn build-client && yarn build-server",
    "build-client": "node build/build-client.js",
    "build-server": "tsc -p tsconfig.server.json --outDir dist/server && node build/build-server.js",
    "build-server-watch": "tsc -p tsconfig.server.json --outDir dist/server ; node build/build-server.js",
    "convert": "src/convert-rpgwo/convert.sh",
    "run-server": "node -r source-map-support/register ./dist/server/server/run.js",
    "run-static-server": "http-server dist/client -c-1",
    "clean": "rimraf dist .cache",
    "watch": "concurrently --kill-others --names Server,Client,Static --prefix name nodemon \"node build/build-client.js --dev\" \"yarn run-static-server > /dev/null\"",
    "watch-no-server": "concurrently --kill-others --names Client,Static --prefix name \"node build/build-client.js --dev\" \"yarn run-static-server > /dev/null\"",
    "watch-ui": "open http://localhost:1234/ui.html && parcel serve src/client/ui.html",
    "type-check": "tsc --noEmit",
    "lint": "eslint '{src,test,build}/**/*.{ts,tsx,js,jsx}'",
    "test": "yarn lint && yarn type-check && jest --testPathIgnorePatterns memory"
  },
  "devDependencies": {
    "@types/d3-delaunay": "^4.1.0",
    "@types/d3-polygon": "^2.0.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^24.0.11",
    "@types/level": "^6.0.0",
    "@types/luxon": "^1.26.5",
    "@types/node-cleanup": "^2.1.1",
    "@types/offscreencanvas": "^2019.6.3",
    "@types/puppeteer": "^1.19.1",
    "@types/seedrandom": "^2.4.28",
    "@types/wicg-file-system-access": "^2020.9.1",
    "@types/ws": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.17.0",
    "@typescript-eslint/parser": "^4.31.0",
    "canvas": "^2.6.1",
    "concurrently": "^4.1.1",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "glob": "^7.1.6",
    "http-server": "^0.11.1",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "puppeteer": "^1.19.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "4.4.2"
  },
  "browserslist": [
    "last 1 Chrome version"
  ]
}
